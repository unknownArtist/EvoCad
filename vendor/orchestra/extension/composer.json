{
	"name": "orchestra/extension",
	"description": "Orchestra Platform 2 Extension Component",
	"homepage": "http://orchestraplatform.com/docs/2.0/components/extension/",
	"keywords": ["orchestra-platform", "orchestral", "laravel", "extension"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com",
			"homepage": "https://github.com/crynobone"
		}
	],
	"autoload": {
		"psr-0": {
			"Orchestra\\Extension" : "src/"
		}
	},
	"require": {
		"php": ">=5.3.3",
		"orchestra/memory": "2.0.*"
	},
	"require-dev": {
		"mockery/mockery": "0.8.0",
		"illuminate/container": "4.0.*",
		"illuminate/http": "4.0.*",
		"illuminate/support": "4.0.*"
	},
	"suggest": {
		"laravel/framework": "Allow using orchestra/extension component outside of Orchestra Platform 2",
		"orchestra/foundation": "Allow using orchestra/extension with Orchestra Platform 2"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.1-dev"
		}
	},
	"minimum-stability": "dev"
}
